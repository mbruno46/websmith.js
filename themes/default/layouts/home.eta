<% layout('./layout') %>

<div class="page-header">
  <div class="row">
    <div class="col-lg-2">
      <% if(it.logo !== undefined) { %>
      <img src="<%~ it.logo %>" style="width: 100%; max-width: 256px">
      <% } %>
    </div>
    <div class="col-lg-8">
      <h1>
        <%= it.title %>
      </h1>
      <p class="lead"><%= it.desc %></p>
    </div>
  </div>
</div>

<div class="bs-docs-section clearfix">
  <%~ it.body %>
</div>
