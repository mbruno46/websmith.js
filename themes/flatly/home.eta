<% layout('./layout') %>

<div class="page-header">
  <h1><%= it.title %>
  <% if(it.logo === undefined) { %>
  </h1>
  <% } else { %>
  <img src="<%= it.logo %>" style="height: 1em;"></h1>
  <% } %>
  <p class="lead"><%= it.desc %></p>
</div>

<div class="bs-docs-section clearfix">
  <%~ it.body %>
</div>
